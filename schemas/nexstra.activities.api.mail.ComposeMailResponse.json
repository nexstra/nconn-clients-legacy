{
  "$schema" : "http://json-schema.org/draft-04/schema#",
  "title" : "Compose Mail Response",
  "type" : "object",
  "additionalProperties" : false,
  "properties" : {
    "rootUri" : {
      "propertyOrder" : 1,
      "type" : "string",
      "title" : "Root Uri"
    },
    "mtime" : {
      "propertyOrder" : 2,
      "type" : "integer",
      "format" : "utc-millisec",
      "title" : "Mtime"
    },
    "docuri" : {
      "propertyOrder" : 3,
      "type" : "string",
      "title" : "Docuri"
    },
    "dir" : {
      "propertyOrder" : 4,
      "type" : "boolean",
      "title" : "Dir"
    },
    "extra" : {
      "propertyOrder" : 5,
      "type" : "object",
      "additionalProperties" : { },
      "title" : "Extra"
    },
    "contentType" : {
      "propertyOrder" : 6,
      "type" : "string",
      "title" : "Content Type"
    },
    "uri" : {
      "propertyOrder" : 7,
      "type" : "string",
      "title" : "Uri"
    },
    "length" : {
      "propertyOrder" : 8,
      "type" : "integer",
      "title" : "Length"
    },
    "name" : {
      "propertyOrder" : 9,
      "type" : "string",
      "title" : "Name"
    },
    "path" : {
      "propertyOrder" : 10,
      "type" : "string",
      "title" : "Path"
    }
  },
  "required" : [ "rootUri", "docuri", "dir", "extra", "contentType", "uri", "name", "path" ]
}